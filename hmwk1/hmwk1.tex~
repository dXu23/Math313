    \text{Let } A_{2} & = & \{1, 2k : k \in \mathbb{N}\} \\
    \text{Let } A_{3} & = & \{3k : k \in \mathbb{N}, k \neq \} \\
    \text{Let } A_{4} & = & \{5k : k \in \mathbb{N}, k \neq 3m, k \neq 2m, m \in \mathbb{N} \} \\
